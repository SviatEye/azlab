{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "sviat-az-lab-df"
		},
		"AzDLake_Lego_Storage_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzDLake_Lego_Storage'"
		},
		"AzureSqlDatabase1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase1'"
		},
		"AzDLake_Lego_Storage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sviatazlbstacc.dfs.core.windows.net"
		},
		"Http_LegoBricks_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://cdn.rebrickable.com/media/downloads/"
		},
		"rs_Dummy_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://dummy.restapiexample.com/api/v1/employees"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "sviat-az-lab-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzDLake_Lego_Storage_accountKey": {
						"value": "[parameters('AzDLake_Lego_Storage_accountKey')]"
					},
					"AzureSqlDatabase1_connectionString": {
						"value": "[parameters('AzureSqlDatabase1_connectionString')]"
					},
					"AzDLake_Lego_Storage_properties_typeProperties_url": {
						"value": "[parameters('AzDLake_Lego_Storage_properties_typeProperties_url')]"
					},
					"Http_LegoBricks_properties_typeProperties_url": {
						"value": "[parameters('Http_LegoBricks_properties_typeProperties_url')]"
					},
					"rs_Dummy_LS_properties_typeProperties_url": {
						"value": "[parameters('rs_Dummy_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}